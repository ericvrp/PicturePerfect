{
  "name": "PicturePerfect",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "list": "^0.8.2",
    "now": "^0.33.1",
    "now-serve": "^0.5.1",
    "react-scripts": "^0.7.0"
  },
  "dependencies": {
    "googleapis": "^14.2.0",
    "react": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-fastclick": "^2.1.2",
    "react-fullscreen": "^0.1.0",
    "react-router": "^3.0.0",
    "react-virtualized": "^8.6.1",
    "sw-precache": "^4.2.3"
  },
  "scripts": {
    "start": "PORT=3002 react-scripts start",
    "start-prod": "npm run build && list -s --port 3002 build",
    "build": "rm -rf build && react-scripts build && sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && now --static build"
  }
}
